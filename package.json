{
  "name": "koffee-digital-website",
  "version": "1.0.0",
  "description": "Professional website for Koffee Digital - Web Development Services",
  "main": "index.html",
  "scripts": {
    "dev": "tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/output.css --watch",
    "build": "tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/output.css --minify",
    "build:prod": "./build.sh",
    "serve": "python3 -m http.server 8000",
    "serve:prod": "cd dist && python3 -m http.server 8000",
    "start": "npm run dev",
    "clean": "rm -rf dist/ node_modules/.cache/",
    "analyze": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/output.css --minify --verbose"
  },
  "keywords": [
    "web-development",
    "small-business",
    "dublin",
    "ireland",
    "tailwindcss"
  ],
  "author": "Koffee Digital",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.4.0"
  }
}